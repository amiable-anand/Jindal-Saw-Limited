{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=JindalGuestHouseDB;Integrated Security=true;TrustServerCertificate=true;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "Security": {
    "RequireHttps": false,
    "AllowedOrigins": [
      "http://localhost:5000",
      "https://localhost:7000",
      "http://localhost:8080",
      "https://localhost:8081"
    ]
  },
  "ApiSettings": {
    "EnableSwagger": true
  }
}
